@model List<CompanyManagement.Models.Transaction>

@foreach (var tran in Model)
{
    <tr>
        <td>@tran.balance</td>
        <td>@tran.income</td>
        <td>@tran.outcome</td>
        <td>@tran.sale_price</td>
        <td>@tran.move_date</td>
        <td>
            <a href="/Orders/Details/@tran.Order.custom_order_id">
                @tran.Order.custom_order_id
            </a>
        </td>
    </tr>
}